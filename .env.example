# PartFinder Pro - Environment Configuration
# Copy this file to .env and fill in your API keys

# OpenAI API Configuration (for AI image recognition and store finding)
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_API_BASE=https://api.openai.com/v1

# Google Places API Configuration (for live store location data)
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Parts Database API (optional - uses local database if not provided)
VITE_PARTS_API_ENDPOINT=https://api.partsdatabase.com
VITE_PARTS_API_KEY=your_parts_api_key_here

# Vendor API Configurations
# RepairClinic API
VITE_REPAIRCLINIC_API_URL=https://api.repairclinic.com/v1
VITE_REPAIRCLINIC_API_KEY=your_repairclinic_api_key_here

# PartSelect API
VITE_PARTSELECT_API_URL=https://api.partselect.com/v1
VITE_PARTSELECT_API_KEY=your_partselect_api_key_here

# AppliancePartsPros API
VITE_APPLIANCEPARTSPROS_API_URL=https://api.appliancepartspros.com/v1
VITE_APPLIANCEPARTSPROS_API_KEY=your_appliancepartspros_api_key_here

# Encompass Supply API
VITE_ENCOMPASS_API_URL=https://api.encompass.com/v1
VITE_ENCOMPASS_API_KEY=your_encompass_api_key_here

# Application Configuration
VITE_APP_MODE=production
VITE_ENABLE_DEMO_MODE=true
VITE_ENABLE_ANALYTICS=true
VITE_CACHE_TIMEOUT=900000

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
VITE_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
VITE_ENABLE_REAL_AI=true
VITE_ENABLE_VENDOR_APIS=true
VITE_ENABLE_CACHING=true
VITE_ENABLE_ERROR_REPORTING=true

